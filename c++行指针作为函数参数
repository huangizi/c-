#include <iostream>
using namespace std;
int main()
{
	void search(float (*p)[4],int n);
	float score[3][4] = {{65,57,70,60},{58,67,80,81},{90,99,100,98}};
	search(score,3);
	return 0;

}

void search(float (*p)[4],int n)
{
	int i,j,flag;
	for(j=0;j<n;j++)
	{
		flag = 0;
		for(i=0;i<4;i++)
			if(*(*(p+j)+i)<60)
				flag = 1;//若有不及格课程，flag=1

		if(flag==1)
		{
			cout << "NO." << j+1 << "fails\n";
			for(i=0;i<4;i++)
			{
				cout << *(*(p+j)+i) << " ";
				cout << endl;
			}
		}
	}
}
